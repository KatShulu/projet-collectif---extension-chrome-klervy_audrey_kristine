{
    "manifest_version": 3,
    "name": "hug",
    "version": "1.0",
    
    "description": "My first attempt at creating a Chrome add-on",
    
    "content_scripts": [ {
        "matches": ["<all_urls>"],
        "js": ["index.js"]
      } ],

      "action": {
        "default_popup": "index.html"
      }

    }