{
  "name": "Vocaboost",
  "version": "1.0.0",
  "description": "Upgrade your languages skills !",
  "manifest_version": 3,
  "author": "klervy, kristine, audrey",
  "action": {
    "default_popup": "index.html",
    "default_title": "VOCABOOST"
  },
  "content_scripts": [
    {
      "matches": ["*://github.com/parcel-bundler/*"],
      "js": ["scripts/index.js"]
    }
  ],
  "host_permissions": ["*://*.wiktionary.org/*"]
}
